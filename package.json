{
    "name": "Team-Management-Dashboard",
    "version": "0.1.0",
    "private": true,
    "main": "index.js",
    "workspaces": [
        "client",
        "server"
    ],
    "scripts": {
        "docker:compose": "docker-compose up -d",
        "client:dev": "npm run dev --workspace=client",
        "server:dev": "npm run start:dev --workspaces=server",
        "start": "concurrently 'npm:server:dev' 'npm:client:dev'",
        "stop": "docker-compose down"
    },
    "dependencies": {
        "@mantine/ds": "^7.2.2",
        "@nestjs/config": "^3.1.1",
        "@nestjs/jwt": "^10.1.1",
        "@nestjs/mongoose": "^10.0.1",
        "@nestjs/passport": "^10.0.2",
        "@nestjs/platform-express": "^10.2.7",
        "@nestjs/platform-socket.io": "^10.2.10",
        "@nestjs/websockets": "^10.2.10",
        "@tabler/icons-react": "^2.40.0",
        "@tiptap/extension-bubble-menu": "^2.2.1",
        "@tiptap/extension-document": "^2.2.1",
        "@tiptap/extension-highlight": "^2.2.1",
        "@tiptap/extension-link": "^2.2.1",
        "@tiptap/extension-placeholder": "^2.2.1",
        "@tiptap/extension-typography": "^2.2.1",
        "@tiptap/pm": "^2.2.1",
        "@tiptap/react": "^2.2.1",
        "@tiptap/starter-kit": "^2.2.1",
        "antd": "^5.13.2",
        "argon2": "^0.31.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "concurrently": "^3.6.1",
        "dotenv": "^16.3.1",
        "i": "^0.3.7",
        "jose": "^5.0.1",
        "lucide-react": "^0.321.0",
        "mongoose": "^7.6.3",
        "next": "^14.1.0",
        "next-themes": "^0.2.1",
        "novel": "^0.1.22",
        "npm": "^9.8.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "react-icons": "^5.0.1",
        "sass": "^1.70.0",
        "typewriter-effect": "^2.21.0",
        "wait-on": "^3.3.0"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.10",
        "@types/passport-jwt": "^3.0.11",
        "autoprefixer": "^10.4.17",
        "postcss": "^8.4.33",
        "tailwindcss": "^3.4.1"
    }
}
